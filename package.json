    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "test-rag-postgres": "ts-node --compilerOptions '{\"module\":\"commonjs\"}' -r tsconfig-paths/register -r dotenv/config scripts/test-postgres.ts dotenv_config_path=.env.local",
